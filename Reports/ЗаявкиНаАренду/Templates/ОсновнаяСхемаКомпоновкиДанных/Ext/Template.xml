	ТипКПП.*,
	ТипПривода.*,
	ТипСалона.*,
	Цвет.*,
	Состояние.*,
	ПричинаОтказаОтОбслуживания.*,
	Организация.*,
	ПодразделениеКомпании.*,
	Менеджер.*,
	Автомобиль.*,
	СуммаВсего,
	СуммаДепозита,
	СуммаНДС,
	СуммаСкидки,
	КоличествоЗаявок.*}
ИЗ
	Документ.ЗаявкаНаАренду КАК ЗаявкаНаАренду
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаявкаНаАренду.Автомобили КАК ЗаявкаНаАрендуАвтомобили
		ПО ЗаявкаНаАренду.Ссылка = ЗаявкаНаАрендуАвтомобили.Ссылка
ГДЕ
