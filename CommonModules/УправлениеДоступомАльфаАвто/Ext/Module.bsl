Процедура СоздатьГруппуДоступа(ИдентификаторПрофиля, РодительГруппыДоступа, ИмяГруппыДоступа = "")
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ГруппыДоступа.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ГруппыДоступа КАК ГруппыДоступа
	|ГДЕ
	|	ГруппыДоступа.Профиль.ИдентификаторПоставляемыхДанных = &Профиль
	|
	|УПОРЯДОЧИТЬ ПО
	|	ГруппыДоступа.ПометкаУдаления
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ ПЕРВЫЕ 1
	|	ПрофилиГруппДоступа.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ПрофилиГруппДоступа КАК ПрофилиГруппДоступа
	|ГДЕ
	|	ПрофилиГруппДоступа.ИдентификаторПоставляемыхДанных = &Профиль";
	
	Запрос.УстановитьПараметр("Профиль", Новый УникальныйИдентификатор(ИдентификаторПрофиля)); 		
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	ГруппыПрофиля = РезультатЗапроса[0].Выгрузить().ВыгрузитьКолонку("Ссылка");
	Профиль = РезультатЗапроса[1].Выгрузить().ВыгрузитьКолонку("Ссылка");
	
	Если ГруппыПрофиля.Количество() > 0 Тогда 
		ГруппаДоступаОбъект = ГруппыПрофиля[0].ПолучитьОбъект();
		ГруппаДоступаОбъект.Родитель = РодительГруппыДоступа;
		ГруппаДоступаОбъект.Записать();
		Возврат;
	ИначеЕсли Профиль.Количество() = 0 Тогда
		Возврат;
	КонецЕсли;
	
	// Создание группы.
	ГруппаДоступаОбъект = Справочники.ГруппыДоступа.СоздатьЭлемент();
	ГруппаДоступаОбъект.Наименование = ?(ЗначениеЗаполнено(ИмяГруппыДоступа), 
										 ИмяГруппыДоступа,
										 Профиль[0].Наименование);
	ГруппаДоступаОбъект.Профиль      = Профиль[0].Ссылка;
	ГруппаДоступаОбъект.Родитель 	 = РодительГруппыДоступа;
	ГруппаДоступаОбъект.Записать(); 
КонецПроцедуры

