		НСтр("ru = 'Базовый профиль для настройки базовых прав пользователей с предыдущей редакции.'");
		ОписанияПрофилей.Добавить(ОписаниеБазовогоПрофиляРедакции);
		
	КонецЕсли;
	
	ПриЗаполненииПоставляемыхОсновныхПрофилейГруппДоступа(ОписанияПрофилей, ПараметрыОбновления);
	ПриЗаполненииПоставляемыхДополнительныхПрофилейГруппДоступа(ОписанияПрофилей, ПараметрыОбновления);
	
КонецПроцедуры

// Добавление командной панели, оформление формы профили групп доступа
//
// Параметры: 
//  Форма - ФормаКлиентскогоПриложения - форма справочника ПрофилиГруппДоступа
//
Процедура ФормаПрофилиГруппДоступаПриСозданииНаСервере(Форма) Экспорт
	
	// Добавим команды
	НоваяКоманда = Форма.Команды.Добавить("СохранитьНастройкиПрофиляВФайл");
	НоваяКоманда.Действие = "Подключаемый_СохранитьНастройкиПрофиляВФайл";
	НоваяКоманда.Заголовок = НСтр("ru = 'Сохранить настройки профиля в файл'");
	НоваяКоманда.Картинка = БиблиотекаКартинок.ЗаписатьИзменения;
	НоваяКоманда.Подсказка = НСтр("ru = 'Сохранить настройки профиля в файл'");
	НоваяКоманда.Отображение = ОтображениеКнопки.Картинка;
	
	НоваяКоманда = Форма.Команды.Добавить("ЗагрузитьНастройкиПрофиляИзФайла");
	НоваяКоманда.Действие = "Подключаемый_ЗагрузитьНастройкиПрофиляИзФайла";
	НоваяКоманда.Заголовок = НСтр("ru = 'Загрузить настройки профиля из файла'");
	НоваяКоманда.Картинка = БиблиотекаКартинок.ПрочитатьИзменения;
	НоваяКоманда.Подсказка = НСтр("ru = 'Загрузить настройки профиля из файла'");
	НоваяКоманда.Отображение = ОтображениеКнопки.Картинка;
	
	// Установим на форме кнопки
	ФормаСохранитьНастройкиПрофиляВФайл = Форма.Элементы.Вставить(
		"ФормаСохранитьНастройкиПрофиляВФайл",
		Тип("КнопкаФормы"),
		Форма.Элементы.ФормаКоманднаяПанель);
	ФормаСохранитьНастройкиПрофиляВФайл.ИмяКоманды = "СохранитьНастройкиПрофиляВФайл";
	
	ФормаЗагрузитьНастройкиПрофиляИзФайла = Форма.Элементы.Вставить(
		"ФормаЗагрузитьНастройкиПрофиляИзФайла",
		Тип("КнопкаФормы"),
		Форма.Элементы.ФормаКоманднаяПанель);
	ФормаЗагрузитьНастройкиПрофиляИзФайла.ИмяКоманды = "ЗагрузитьНастройкиПрофиляИзФайла";
	
	// Оформление элементов формы
	Форма.Элементы.ОтображениеРолей.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	Форма.Элементы.ВидыИЗначенияДоступа.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	Форма.Элементы.СтраницаОписание.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	Форма.Элементы.СтраницаКомментарий.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	
КонецПроцедуры

// Оформление элементво формы группы доступа 
// 
// Параметры:
//  Форма - ФормаКлиентскогоПриложения - форма справочника ПрофилиГруппДоступа 
//
Процедура ФормаГруппыДоступаПриСозданииНаСервере(Форма) Экспорт
	
	// Оформление элементов формы
	Форма.Элементы.ПользователиГруппы.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	Форма.Элементы.Доступ.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	Форма.Элементы.СтраницаКомментарий.ЦветФона = ЦветаСтиля.ЦветОсновнойПанелиФормы;
	
КонецПроцедуры

// Определяет, является ли переданный тип примитивным типом: строка, число, дата, булево.
//
// Параметры:
//  ТипОбъекта- Тип
//
// Возвращаемое значение:
//  Булево.
//
Функция ЭтоПримитивныйТип(ТипОбъекта) Экспорт
	
	ПримитивныеТипы = Новый Массив;
	ПримитивныеТипы.Добавить(Тип("Булево"));
	ПримитивныеТипы.Добавить(Тип("Дата"));
	ПримитивныеТипы.Добавить(Тип("Число"));
	ПримитивныеТипы.Добавить(Тип("Строка"));
	
	Возврат (ПримитивныеТипы.Найти(ТипОбъекта) <> Неопределено);
	
КонецФункции

// Получает Пустое значение примитивного типа по типу
//
// Параметры:
//  ТипОбъекта - Тип - Тип
// 
// Возвращаемое значение:
//   Булево, Дата, Число, Строка - 0, 1.1.0001, "", Ложь.
//
Функция ПустоеЗначениеПримитивногоТипа(ТипОбъекта) Экспорт
	
	Если ТипОбъекта = Тип("Булево") Тогда
		Возврат Ложь;
	ИначеЕсли ТипОбъекта = Тип("Дата") Тогда
		Возврат Дата(1,1,1);
	ИначеЕсли ТипОбъекта = Тип("Число") Тогда
		Возврат 0;
	ИначеЕсли ТипОбъекта = Тип("Строка") Тогда
		Возврат "";
	КонецЕсли;
	
КонецФункции

// Заполняет признак "Включая нижестоящие"
//
Процедура ЗаполнитьПризнакВключаяНижестоящие() Экспорт
	
	// Пройдемся по профилям и по группам доступа
	УстановитьПривилегированныйРежим(Истина);
	
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ГруппыДоступаЗначенияДоступа.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ГруппыДоступа.ЗначенияДоступа КАК ГруппыДоступаЗначенияДоступа
	|ГДЕ
	|	ГруппыДоступаЗначенияДоступа.УдалитьДоступПоИерархии
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ПрофилиГруппДоступаЗначенияДоступа.Ссылка КАК Ссылка
	|ИЗ
	|	Справочник.ПрофилиГруппДоступа.ЗначенияДоступа КАК ПрофилиГруппДоступаЗначенияДоступа
	|ГДЕ
	|	ПрофилиГруппДоступаЗначенияДоступа.УдалитьДоступПоИерархии";
	
	ПакетЗапроса = Запрос.ВыполнитьПакет();
	ВыборкаГруппыПрофиля = ПакетЗапроса[0].Выбрать();
	
	СтруктураПоиска = Новый Структура("УдалитьДоступПоИерархии", Истина);
	
	// Сначала Группы доступа
	Пока ВыборкаГруппыПрофиля.Следующий() Цикл
		
		ГруппаДоступаОбъект = ВыборкаГруппыПрофиля.Ссылка.ПолучитьОбъект();
		НайденныеСтроки = ГруппаДоступаОбъект.ЗначенияДоступа.НайтиСтроки(СтруктураПоиска);
		
		Для Каждого ТекущаяСтрока Из НайденныеСтроки Цикл
			ТекущаяСтрока.ВключаяНижестоящие = Истина;
			ТекущаяСтрока.УдалитьДоступПоИерархии = Ложь;
		КонецЦикла;
		
		ГруппаДоступаОбъект.ОбменДанными.Загрузка = Истина;
		
		Попытка
			
			ГруппаДоступаОбъект.Записать();
			
		Исключение
			
			ИмяСобытия = НСтр("ru = 'Заполнение настройки ""Включая нижестоящие"" у группы доступа.'",
				ОбщегоНазначения.КодОсновногоЯзыка());
			ЗаписьЖурналаРегистрации(
				ИмяСобытия,
				УровеньЖурналаРегистрации.Ошибка,
				,
				,
				ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке())
			);
			Продолжить;
			
		КонецПопытки;
		
	КонецЦикла;
	
	// Выберем профили групп доступа
	ВыборкаПрофиля = ПакетЗапроса[1].Выбрать();
	Пока ВыборкаПрофиля.Следующий() Цикл
		
		ПрофильОбъект = ВыборкаПрофиля.Ссылка.ПолучитьОбъект();
		НайденныеСтроки = ПрофильОбъект.ЗначенияДоступа.НайтиСтроки(СтруктураПоиска);
		
		Для Каждого ТекущаяСтрока Из НайденныеСтроки Цикл
			ТекущаяСтрока.ВключаяНижестоящие = Истина;
			ТекущаяСтрока.УдалитьДоступПоИерархии = Ложь;
		КонецЦикла;
		
		ПрофильОбъект.ОбменДанными.Загрузка = Истина;
		
		Попытка
			
			ПрофильОбъект.Записать();
			
		Исключение
			
			ИмяСобытия = НСтр("ru = 'Заполнение настройки ""Включая нижестоящие"" у профиля группы доступа.'",
				ОбщегоНазначения.КодОсновногоЯзыка());
			ЗаписьЖурналаРегистрации(
				ИмяСобытия,
				УровеньЖурналаРегистрации.Ошибка,
				,
				,
				ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке())
			);
			Продолжить;
			
		КонецПопытки;
		
	КонецЦикла;
	
КонецПроцедуры

// Создание групп доступа по настройки профилей из соответсвующих элементов справочника
//
Процедура СозданиеГруппДоступаПоПрофилям() Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	ГруппыДоступаУдалитьПрофилиДоступа.Ссылка КАК Ссылка
		|ИЗ
		|	Справочник.ГруппыДоступа КАК ГруппыДоступаУдалитьПрофилиДоступа
		|ГДЕ
		|	ГруппыДоступаУдалитьПрофилиДоступа.Ссылка <> ЗНАЧЕНИЕ(Справочник.ГруппыДоступа.Администраторы)
		|	И НЕ ГруппыДоступаУдалитьПрофилиДоступа.ЭтоГруппа
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ГруппыДоступа.Ссылка КАК ГруппаДоступа,
		|	ГруппыДоступа.Профиль КАК Профиль
		|ПОМЕСТИТЬ ГруппыДоступаСПрофилем
		|ИЗ
		|	Справочник.ГруппыДоступа КАК ГруппыДоступа
		|ГДЕ
		|	ГруппыДоступа.Профиль <> ЗНАЧЕНИЕ(Справочник.ПрофилиГруппДоступа.ПустаяСсылка)
		|	И ГруппыДоступа.Ссылка <> ЗНАЧЕНИЕ(Справочник.ГруппыДоступа.Администраторы)
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ГруппыДоступаСПрофилем.Профиль КАК Профиль,
		|	ГруппыДоступаВидыДоступа.ВидДоступа КАК ВидДоступа,
		|	ГруппыДоступаВидыДоступа.ВсеРазрешены КАК ВсеРазрешены,
		|	ГруппыДоступаСПрофилем.ГруппаДоступа КАК ГруппаДоступа
		|ПОМЕСТИТЬ ТаблицаВидовДоступов
		|ИЗ
		|	ГруппыДоступаСПрофилем КАК ГруппыДоступаСПрофилем
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа.ВидыДоступа КАК ГруппыДоступаВидыДоступа
		|		ПО ГруппыДоступаСПрофилем.ГруппаДоступа = ГруппыДоступаВидыДоступа.Ссылка
		|			И ГруппыДоступаСПрофилем.Профиль = ГруппыДоступаВидыДоступа.УдалитьПрофиль
		|ГДЕ
		|	ГруппыДоступаВидыДоступа.ВидДоступа <> НЕОПРЕДЕЛЕНО
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ГруппыДоступаСПрофилем.Профиль КАК Профиль,
		|	ГруппыДоступаСПрофилем.ГруппаДоступа КАК ГруппаДоступа,
		|	ГруппыДоступаЗначенияДоступа.ВидДоступа КАК ВидДоступа,
		|	ГруппыДоступаЗначенияДоступа.ЗначениеДоступа КАК ЗначениеДоступа,
		|	ГруппыДоступаЗначенияДоступа.ВключаяНижестоящие КАК ВключаяНижестоящие
		|ПОМЕСТИТЬ ТаблицаЗначийВидовДоступа
		|ИЗ
		|	ГруппыДоступаСПрофилем КАК ГруппыДоступаСПрофилем
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа.ЗначенияДоступа КАК ГруппыДоступаЗначенияДоступа
		|		ПО ГруппыДоступаСПрофилем.ГруппаДоступа = ГруппыДоступаЗначенияДоступа.Ссылка
		|			И ГруппыДоступаСПрофилем.Профиль = ГруппыДоступаЗначенияДоступа.УдалитьПрофиль
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	ТаблицаВидовДоступов.Профиль КАК Профиль,
		|	ТаблицаВидовДоступов.ВидДоступа КАК ВидДоступа,
		|	ТаблицаВидовДоступов.ВсеРазрешены КАК ВсеРазрешены,
		|	ТаблицаВидовДоступов.ГруппаДоступа КАК ГруппаДоступа,
		|	ЕСТЬNULL(ТаблицаЗначийВидовДоступа.ЗначениеДоступа, НЕОПРЕДЕЛЕНО) КАК ЗначениеДоступа,
		|	ЕСТЬNULL(ТаблицаЗначийВидовДоступа.ВключаяНижестоящие, НЕОПРЕДЕЛЕНО) КАК ВключаяНижестоящие
		|ИЗ
		|	ТаблицаВидовДоступов КАК ТаблицаВидовДоступов
		|		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаЗначийВидовДоступа КАК ТаблицаЗначийВидовДоступа
		|		ПО ТаблицаВидовДоступов.Профиль = ТаблицаЗначийВидовДоступа.Профиль
		|			И ТаблицаВидовДоступов.ВидДоступа = ТаблицаЗначийВидовДоступа.ВидДоступа
		|			И ТаблицаВидовДоступов.ГруппаДоступа = ТаблицаЗначийВидовДоступа.ГруппаДоступа";
	
	ПакетЗапроса = Запрос.ВыполнитьПакет();
	
	ПрофилиГрупп = ПакетЗапроса[0].Выбрать();
	
	// Нет профилей для перевода
	Если ПрофилиГрупп.Количество() = 0 Тогда 
		Возврат;
	КонецЕсли;
	
	// Создам группу для новых групп доступов
	ИмяГруппы = НСтр("ru = 'Группы доступа из предыдущей редакции'");
	ОбщаяГруппаДоступа = Справочники.ГруппыДоступа.НайтиПоНаименованию(ИмяГруппы);
	
	Если НЕ ЗначениеЗаполнено(ОбщаяГруппаДоступа) Тогда
		ОбщаяГруппаДоступаОбъект = Справочники.ГруппыДоступа.СоздатьГруппу();
		ОбщаяГруппаДоступаОбъект.Наименование = ИмяГруппы;
		ОбщаяГруппаДоступаОбъект.Записать();
		ОбщаяГруппаДоступа = ОбщаяГруппаДоступаОбъект.Ссылка;
	КонецЕсли;
	
	ТаблицаВидовДоступаПоПрофилю = ПакетЗапроса[4].Выгрузить();
	
	СписокПрофилейИГруппДоступа = ТаблицаВидовДоступаПоПрофилю.Скопировать();
	СписокПрофилейИГруппДоступа.Свернуть("Профиль,ГруппаДоступа");
	
	ПрофилиДляОбновленияПользователей = Новый Массив();
	
	Пока ПрофилиГрупп.Следующий() Цикл
		
		// Получим объект группы доступа
		ОбъектГруппыДоступа = ПрофилиГрупп.Ссылка.ПолучитьОбъект();
		
		// Нет в табличной части профилей 
		Отказ = Ложь;
		Если ОбъектГруппыДоступа.УдалитьПрофилиДоступа.Количество() > 0 Тогда
			
			СопоставитьГруппуДоступаПрофилю(
				ОбъектГруппыДоступа,
				ТаблицаВидовДоступаПоПрофилю,
				СписокПрофилейИГруппДоступа,
				ОбщаяГруппаДоступа,
				Отказ
			);
			
		// Профиль у группы уже заполнен, делать ничего не надо с группой доступа
		ИначеЕсли ЗначениеЗаполнено(ОбъектГруппыДоступа.Профиль) Тогда
			
			Отказ = Истина;
			
		КонецЕсли;
		
		Если Отказ Тогда
			Продолжить;
		КонецЕсли;
		
		// Для групп доступа без профилей почистим пользователей и установим пометку на удаление.
		ОбъектГруппыДоступа.Пользователи.Очистить();
		ОбъектГруппыДоступа.УдалитьПрофилиДоступа.Очистить();
		ОбъектГруппыДоступа.Пользователь = Справочники.Пользователи.ПустаяСсылка();
		ОбъектГруппыДоступа.ПометкаУдаления = Истина;
		
		ОбъектГруппыДоступа.ДополнительныеСвойства.Вставить("НеОбновлятьРолиПользователей");
		ПрофилиДляОбновленияПользователей.Добавить(ПрофилиГрупп.Ссылка);
		
		Попытка
			
			ОбъектГруппыДоступа.Записать();
			
		Исключение
			
			ИмяСобытия = НСтр("ru = 'Запись группы доступа после изменения.'", ОбщегоНазначения.КодОсновногоЯзыка());
			ЗаписьЖурналаРегистрации(
				ИмяСобытия,
				УровеньЖурналаРегистрации.Ошибка,
				,
				,
				ОбработкаОшибок.ПодробноеПредставлениеОшибки(ИнформацияОбОшибке())
			);
			Продолжить;
			
		КонецПопытки;
		
	КонецЦикла;
	
	ПользователиДляОбновленияРолей = Справочники.ГруппыДоступа.ПользователиДляОбновленияРолейПоПрофилю(
		ПрофилиДляОбновленияПользователей);
	УправлениеДоступом.ОбновитьРолиПользователей(ПользователиДляОбновленияРолей);
	
КонецПроцедуры 

// Создание группы доступа "Базовый профиль" с заполнением профиля и пользователей.
//
Процедура ДобавитьБазовыйПрофильПользователям() Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	// Найдем базовый профиль
	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ ПЕРВЫЕ 1
	               |	ПрофилиГруппДоступа.Ссылка КАК Ссылка
	               |ПОМЕСТИТЬ БазовыйПрофиль
	               |ИЗ
	               |	Справочник.ПрофилиГруппДоступа КАК ПрофилиГруппДоступа
	               |ГДЕ
	               |	ПрофилиГруппДоступа.ИдентификаторПоставляемыхДанных = &ИдентификаторПоставляемыхДанных
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	БазовыйПрофиль.Ссылка КАК БазовыйПрофиль,
	               |	БазовыйПрофиль.Ссылка.Наименование КАК НаименованиеБазовогоПрофиля,
	               |	ГруппыДоступа.Ссылка КАК ГруппаДоступа
	               |ИЗ
	               |	БазовыйПрофиль КАК БазовыйПрофиль
	               |		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ГруппыДоступа КАК ГруппыДоступа
	               |		ПО БазовыйПрофиль.Ссылка = ГруппыДоступа.Профиль
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	ГруппыДоступаПользователи.Пользователь КАК Пользователь
	               |ПОМЕСТИТЬ Администраторы
	               |ИЗ
	               |	Справочник.ГруппыДоступа.Пользователи КАК ГруппыДоступаПользователи
	               |ГДЕ
	               |	ГруппыДоступаПользователи.Ссылка = ЗНАЧЕНИЕ(Справочник.ГруппыДоступа.Администраторы)
	               |;
	               |
	               |////////////////////////////////////////////////////////////////////////////////
	               |ВЫБРАТЬ
	               |	Пользователи.Ссылка КАК Пользователь
	               |ИЗ
	               |	Справочник.Пользователи КАК Пользователи
	               |ГДЕ
	               |	НЕ Пользователи.Ссылка В
	               |				(ВЫБРАТЬ
	               |					Администраторы.Пользователь КАК Пользователь
	               |				ИЗ
	               |					Администраторы КАК Администраторы)";
	Запрос.УстановитьПараметр(
		"ИдентификаторПоставляемыхДанных",
		Новый УникальныйИдентификатор("62a441f0-02e0-432a-a7bc-087a6a04c919"));
	
	ПакетЗапроса = Запрос.ВыполнитьПакет();
	
	ВыборкаДоступа = ПакетЗапроса[1].Выбрать();
	Если НЕ ВыборкаДоступа.Следующий() Тогда
		Возврат;
	КонецЕсли;
	
	Если ВыборкаДоступа.ГруппаДоступа = Null Тогда
		ГруппаДоступа = Справочники.ГруппыДоступа.СоздатьЭлемент();
		ГруппаДоступа.Наименование = ВыборкаДоступа.НаименованиеБазовогоПрофиля;
		ГруппаДоступа.Профиль = ВыборкаДоступа.БазовыйПрофиль;
		ГруппаДоступа.Комментарий = НСтр("ru = 'Создана автоматически для описания базовых прав пользователям'");
	Иначе
		ГруппаДоступа = ВыборкаДоступа.ГруппаДоступа.ПолучитьОбъект();
	КонецЕсли;
	
	ВыборкаПользователей = ПакетЗапроса[3].Выбрать();
	
	СтруктураПоиска = Новый Структура("Пользователь");
	Пока ВыборкаПользователей.Следующий() Цикл
		СтруктураПоиска.Пользователь = ВыборкаПользователей.Пользователь;
		НайденныеСтроки = ГруппаДоступа.Пользователи.НайтиСтроки(СтруктураПоиска);
		Если НайденныеСтроки.Количество() = 0 Тогда
			НоваяСтрока = ГруппаДоступа.Пользователи.Добавить();
			НоваяСтрока.Пользователь = ВыборкаПользователей.Пользователь;
		КонецЕсли;
	КонецЦикла;
	
	ГруппаДоступа.Записать();
	
КонецПроцедуры

