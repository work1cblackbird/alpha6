		ОбщаяГруппаДоступа,
		ЕстьОшибки)
	
	ТаблицаПоискаТекущегоПрофиля = Новый ТаблицаЗначений();
	ТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("ВидДоступа");
	ТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("ВсеРазрешены");
	ТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("ЗначениеДоступа");
	ТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("ВключаяНижестоящие");
	
	// Проверим каждый профиль и при необходимоти создадим новый
	Для Каждого ТекущийПрофиль Из ГруппаДоступа.УдалитьПрофилиДоступа Цикл
		
		// Нельзя администратора указывать, кроме группы доступа Администраторы
		Если ТекущийПрофиль.Профиль = Справочники.ПрофилиГруппДоступа.Администратор Тогда
			Продолжить;
		КонецЕсли;
		
		ТаблицаПоискаТекущегоПрофиля.Очистить();
		
		ЗаполнитьВидыДоступаГруппыПоПрофилю(
			ТаблицаПоискаТекущегоПрофиля,
			ГруппаДоступа,
			ТекущийПрофиль.Профиль);
		
		Если ЗаполненаГруппаДоступаПоПрофилю(
			ГруппаДоступа,
			ТекущийПрофиль.Профиль,
			СписокПрофилейИГруппДоступа,
			ТаблицаВидовДоступаПоПрофилю,
			ТаблицаПоискаТекущегоПрофиля,
			ЕстьОшибки) Тогда
			Продолжить;
		КонецЕсли;
		
		// Создаем новую группу доступа по профилю
		ОбъектПрофиляГруппыДоступа = ТекущийПрофиль.Профиль.ПолучитьОбъект();
		НоваяГруппаДоступа = Справочники.ГруппыДоступа.СоздатьЭлемент();
		НоваяГруппаДоступа.Наименование = СтрШаблон(
			НСтр("ru = '%1 по группе доступа %2'"),
			ОбъектПрофиляГруппыДоступа.Наименование,
			ГруппаДоступа.Наименование);
		НоваяГруппаДоступа.Профиль = ТекущийПрофиль.Профиль;
		НоваяГруппаДоступа.Ответственный = ГруппаДоступа.Ответственный;
		НоваяГруппаДоступа.Комментарий = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
			НСтр("ru = 'Создана автоматически при переходе на новую редакцию по группе доступа <%1> с профилем <%2>'"),
			ГруппаДоступа.Наименование,
			ОбъектПрофиляГруппыДоступа.Наименование);
		НоваяГруппаДоступа.Родитель = ОбщаяГруппаДоступа;
		
		// Заполним пользователей по текущей группе доступа
		НоваяГруппаДоступа.Пользователи.Загрузить(ГруппаДоступа.Пользователи.Выгрузить());
		
		СтруктураОтбора = Новый Структура("ВидДоступа");
		Для Каждого ТекущаяСтрока Из ТаблицаПоискаТекущегоПрофиля Цикл
			
			СтруктураОтбора.ВидДоступа = ТекущаяСтрока.ВидДоступа;
			НайденныеСтроки = НоваяГруппаДоступа.ВидыДоступа.НайтиСтроки(СтруктураОтбора);
			
			Если НайденныеСтроки.Количество() = 0 Тогда
				НоваяСтрока = НоваяГруппаДоступа.ВидыДоступа.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрока);
			КонецЕсли;
			
			Если ТекущаяСтрока.ЗначениеДоступа = Неопределено Тогда
				Продолжить;
			КонецЕсли;
			
			НоваяСтрока = НоваяГруппаДоступа.ЗначенияДоступа.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрока);
			
		КонецЦикла;
		
		Попытка
			НоваяГруппаДоступа.ДополнительныеСвойства.Вставить("НеОбновлятьРолиПользователей");
			НоваяГруппаДоступа.Записать();
		Исключение
			ИмяСобытия =
				НСтр("ru = 'Запись группы доступа на основании профиля доступа.'",
				ОбщегоНазначения.КодОсновногоЯзыка());
			ЗаписьЖурналаРегистрации(
				ИмяСобытия,
				УровеньЖурналаРегистрации.Ошибка,,,
				ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
			ЕстьОшибки = Истина;
			Продолжить;
		КонецПопытки;
		
		КопияТаблицаПоискаТекущегоПрофиля = ТаблицаПоискаТекущегоПрофиля.Скопировать();
		КопияТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("ГруппаДоступа");
		КопияТаблицаПоискаТекущегоПрофиля.Колонки.Добавить("Профиль");
		
		Для Каждого ТекущаяСтрока Из КопияТаблицаПоискаТекущегоПрофиля Цикл
			ТекущаяСтрока.ГруппаДоступа = НоваяГруппаДоступа.Ссылка;
			ТекущаяСтрока.Профиль = ТекущийПрофиль.Профиль;
		КонецЦикла;
		
		НоваяСтрока = СписокПрофилейИГруппДоступа.Добавить();
		НоваяСтрока.Профиль = ТекущийПрофиль.Профиль;
		НоваяСтрока.ГруппаДоступа = НоваяГруппаДоступа.Ссылка;
		
		ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(
			КопияТаблицаПоискаТекущегоПрофиля,
			ТаблицаВидовДоступаПоПрофилю);
		
	КонецЦикла;
	
	РегистрыСведений.ЗначенияГруппДоступа.ОбновитьДанныеРегистра();
	
КонецПроцедуры

Функция ЗаполненаГруппаДоступаПоПрофилю(
		ГруппаДоступа,
		Профиль,
		СписокПрофилейИГруппДоступа,
		ТаблицаВидовДоступаПоПрофилю,
		ТаблицаПоискаТекущегоПрофиля,
		ЕстьОшибки)
	
	СтруктураПоискаПоПрофилюГруппеДоступа = Новый Структура("Профиль,ГруппаДоступа");
	ТаблицаПоискаПрофиля = ТаблицаПоискаТекущегоПрофиля.Скопировать();
	
	// Попробуем найти текущий профиль в уже сформированных
	ГруппыДоступаПрофиля = СписокПрофилейИГруппДоступа.НайтиСтроки(
		Новый Структура("Профиль", Профиль));
	
	НеНайденаГруппаДоступа = Истина;
	
	Для Каждого ТекущаяГруппаПрофиля Из ГруппыДоступаПрофиля Цикл
		
		ЗаполнитьЗначенияСвойств(СтруктураПоискаПоПрофилюГруппеДоступа, ТекущаяГруппаПрофиля);
		НайденныйПрофиль = ТаблицаВидовДоступаПоПрофилю.НайтиСтроки(СтруктураПоискаПоПрофилюГруппеДоступа);
		ТаблицаПоискаПрофиля.Очистить();
		
		Для Каждого ТекущаяСтрока Из НайденныйПрофиль Цикл
			ЗаполнитьЗначенияСвойств(ТаблицаПоискаПрофиля.Добавить(), ТекущаяСтрока);
		КонецЦикла; 
		
		Если ОбщегоНазначения.КоллекцииИдентичны(ТаблицаПоискаПрофиля, ТаблицаПоискаТекущегоПрофиля) Тогда
			НеНайденаГруппаДоступа = Ложь;
			ОбъектГруппыДоступа = ТекущаяГруппаПрофиля.ГруппаДоступа.ПолучитьОбъект();
			
			// Дополним пользователями группу доступа
			Для Каждого ТекущийПользователь Из ГруппаДоступа.Пользователи Цикл
				НайденныеСтроки = ОбъектГруппыДоступа.Пользователи.НайтиСтроки(
					Новый Структура("Пользователь", ТекущийПользователь.Пользователь)); 
				Если НайденныеСтроки.Количество() = 0 Тогда
					НоваяСтрока = ОбъектГруппыДоступа.Пользователи.Добавить();
					НоваяСтрока.Пользователь = ТекущийПользователь.Пользователь;
				КонецЕсли;
			КонецЦикла;
			
			Попытка
				ОбъектГруппыДоступа.ДополнительныеСвойства.Вставить("НеОбновлятьРолиПользователей");
				ОбъектГруппыДоступа.Записать();
			Исключение
				ИмяСобытия =
					НСтр("ru = 'Запись группы доступа на основании профиля доступа.'",
					ОбщегоНазначения.КодОсновногоЯзыка());
				ЗаписьЖурналаРегистрации(
					ИмяСобытия,
					УровеньЖурналаРегистрации.Ошибка,,,
					ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));
				ЕстьОшибки = Истина;
			КонецПопытки;
			Возврат Истина;
			
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат Ложь;
	
КонецФункции

Процедура ЗаполнитьВидыДоступаГруппыПоПрофилю(ТаблицаПоискаТекущегоПрофиля, ГруппаДоступа, Профиль)
	
	// Заполним данные текущего профиля
	СтруктураПоискаПоПрофилю = Новый Структура();
	СтруктураПоискаПоПрофилю.Вставить("УдалитьПрофиль", Профиль);
	СтруктураПоискаПоВидуДоступа = Новый Структура("УдалитьПрофиль,ВидДоступа");
	СтруктураПоискаПоВидуДоступа.УдалитьПрофиль = Профиль;
	
	ВидыДоступаГруппы = ГруппаДоступа.ВидыДоступа.НайтиСтроки(СтруктураПоискаПоПрофилю);
	
	Для Каждого ТекущийВидДоступа Из ВидыДоступаГруппы Цикл
		Если ТекущийВидДоступа.ВидДоступа = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		СтруктураПоискаПоВидуДоступа.ВидДоступа = ТекущийВидДоступа.ВидДоступа;
		ЗначенияДоступаГруппы = ГруппаДоступа.ЗначенияДоступа.НайтиСтроки(СтруктураПоискаПоВидуДоступа);
		
		Если ЗначенияДоступаГруппы.Количество() = 0 Тогда
			НоваяСтрока = ТаблицаПоискаТекущегоПрофиля.Добавить();
			НоваяСтрока.ВидДоступа = ТекущийВидДоступа.ВидДоступа;
			НоваяСтрока.ВсеРазрешены = ТекущийВидДоступа.ВсеРазрешены;
			НоваяСтрока.ЗначениеДоступа = Неопределено;
			НоваяСтрока.ВключаяНижестоящие = Неопределено;
		КонецЕсли;
		
		Для Каждого ТекущаяСтрока Из ЗначенияДоступаГруппы Цикл
			НоваяСтрока = ТаблицаПоискаТекущегоПрофиля.Добавить();
			НоваяСтрока.ВидДоступа = ТекущаяСтрока.ВидДоступа;
			НоваяСтрока.ВсеРазрешены = ТекущийВидДоступа.ВсеРазрешены;
			НоваяСтрока.ЗначениеДоступа = ТекущаяСтрока.ЗначениеДоступа;
			НоваяСтрока.ВключаяНижестоящие = ТекущаяСтрока.ВключаяНижестоящие;
		КонецЦикла;
		
	КонецЦикла;
	
КонецПроцедуры

#КонецОбласти