	Если ТипЗнч(Объект.ДокументОснование)=Тип("ДокументСсылка.ЗаказВнутренний") Тогда
		Объект.Контрагент = ?(ЗначениеЗаполнено(Объект.ДокументОснование), Объект.ДокументОснование.ПодразделениеПолучатель, Неопределено);
	ИначеЕсли ТипЗнч(Объект.ДокументОснование)=Тип("ДокументСсылка.ЗаказПокупателя") Тогда
		Объект.Контрагент = Объект.ДокументОснование.Контрагент;
	КонецЕсли;
	
КонецПроцедуры

